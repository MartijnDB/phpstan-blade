{
    "name": "thibaud-dauce/phpstan-blade",
    "description": "PHPStan extension to analyse Laravel Blade views.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ThibaudDauce\\PHPStanBlade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/phpstan/phpstan-src.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "laravel/framework": "^9.0 || ^10.0",
        "phpstan/phpstan": "^1.4",
        "nikic/php-parser": "^4.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/process": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThibaudDauce\\PHPStanBlade\\ServiceProvider"
            ]
        }
    }
}
